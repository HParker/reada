<table class="table-minimal">
  <tbody>
    <tr>
      <th>Name</th>
      <th>Articles</th>
      <th>Status</th>
    </tr>
    <% @feeds.each do |feed| %>
      <tr>
        <td><%= link_to feed.title, feed %></td>
        <td><%= feed.stories.count %></td>
        <td><span class="badge-<%= feed.status %>"><%= feed.status%></span></td>
      </tr>
    <% end %>
  </tbody>
</table>
